<UserControl x:Class="WelldonePOS.UserControls.Misc.AppLogo"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Grid Style="{StaticResource grdAppLogo_1}">
        
        <StackPanel Orientation="Horizontal">
            <Grid>
                <Rectangle Style="{StaticResource rctAppLogo_1}"/>
                <TextBlock Style="{StaticResource tblAppLogo_1}"/>
            </Grid>
            <StackPanel>
                <TextBlock Style="{StaticResource tblAppLogo_2}"/>
                <TextBlock Style="{StaticResource tblAppLogo_3}"/>
            </StackPanel>
        </StackPanel>
        
    </Grid>
</UserControl>
