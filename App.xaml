<Application x:Class="WelldonePOS.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainShell.xaml">
    <Application.Resources>
        
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Converters.xaml"/>
                <ResourceDictionary Source="Resources/Vectors.xaml"/>
                <ResourceDictionary Source="Resources/StylesCommon.xaml"/>
                <ResourceDictionary Source="Resources/StylesUnique.xaml"/>
                <ResourceDictionary Source="Resources/ObjectDataProviders.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
        
    </Application.Resources>
</Application>
